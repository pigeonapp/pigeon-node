{
  "extends": ["eslint-config-airbnb-base", "eslint-config-prettier"],
  "plugins": ["eslint-plugin-prettier", "eslint-plugin-import"],
  "rules": {
    "global-require": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
    "no-path-concat": 0,
    "quotes": ["error", "single"]
  }
}
